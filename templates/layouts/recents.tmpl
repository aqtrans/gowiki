{{ define "title" }}<title>{{.Data.SiteName}}|List</title>{{ end }}
{{ define "content" }}
<main>
<article>
  <header>Recent Activity</header> 
</article>    
<article>
    <table>
    <thead>
        <tr>
        <th>Date</th>
        <th>Commit</th>
        <th>Filenames</th>
        </tr>
    </thead>
    <tbody>
    {{ $user := .Data.UN| isLoggedIn }}
    {{ $admin := .Data.IsAdmin }}
    {{range .Data.Recents}}
        <tr>
        <td>{{.Date | prettyDate}}</td>
        <td>{{.Commit}}</td>
        <td>{{range .Filenames}}  <a href="/{{.}}">{{.}}</a>{{end}}</td>
        </tr>
    {{ end }}
    </tbody>
    </table>    
</article>
</main>
{{ end }}