{{ define "title" }}Tag List{{ end }}
{{ define "content" }}
<ul class="tabs">
    <li class="tabs-title is-active"><a href="#">{{ svg "price-tags" }} Tag List</a></li>
</ul>

<main>
    <table>
    <thead>
        <tr>
        <th>Tag</th>
        <th>Pages</th>
        </tr>
    </thead>
    <tbody>
    {{range $index, $element := .TagKeys}}
        <tr>
            <td><a href="/tag/{{ $index }}">{{ $index }}</td>
            <td>{{ range $element }}<a href="/{{ . }}">[{{ . }}]</a>&nbsp;{{ end }}</td> 
        </tr>
    {{ end }}
    </tbody>
    </table>
</main>
{{ end }}