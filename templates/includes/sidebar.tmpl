{{ define "sidebar" }}
<fieldset class="fieldset">
  <legend>Wiki</legend>
<ul>
    <li><a href="/">Home</a></li>
    <li><a href="/list">List</a></li>
    <li><a href="/tags">Tags</a></li>
    <li><a href="/gitadd">Git Add</a></li>
</ul>
</fieldset>
<fieldset class="fieldset">
  <legend>Favs</legend>
<ul>
{{range .page.Favs}}
<li><a href="/{{.}}">{{.}}</a></li>
{{end}}
</ul>
</fieldset>

<form method="get" action="/new" id="newwiki">
        <input type="text" placeholder="Add page" id="newwiki" name="newwiki">
        <input type="hidden" name="token" value="{{.Token}}">
        <div>
        <button type="button" class="success button"><i class="fa fa-plus-square-o fa-lg" aria-hidden="true"></i></button>
        </div>   
</form>

    {{ template "usernav" .}}
{{ end }}